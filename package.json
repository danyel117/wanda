{
  "name": "wanda",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint components/** utils/** pages/**",
    "lint:fix": "eslint --fix components/** utils/** pages/**",
    "generate_build": "cosmo && npx prisma generate && prettier --config .prettierrc prisma/generated/graphql/**/*.ts --write && next build",
    "aurora": "aurora && npx prisma format",
    "cosmo": "cosmo && prettier --config .prettierrc prisma/generated/graphql/**/*.ts --write",
    "generate": "yarn aurora && npx prisma generate && yarn cosmo",
    "migrate-dev": "yarn aurora && npx prisma migrate dev",
    "migrate-deploy": "yarn aurora && npx prisma migrate deploy"
  },
  "dependencies": {
    "@apollo/client": "^3.6.8",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.8.4",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prevalentware/prisma-cosmo": "0.1.11",
    "@prisma/client": "^3.15.1",
    "apollo-server-micro": "^3.9.0",
    "apollo3-cache-persist": "^0.14.0",
    "aws-sdk": "^2.1157.0",
    "graphql": "^16.5.0",
    "micro": "^9.3.4",
    "nanoid": "^4.0.0",
    "next": "12.1.6",
    "next-auth": "^4.5.0",
    "next-superjson": "^0.0.3",
    "nextjs-cors": "^2.1.1",
    "prisma-aurora": "^1.3.9",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-draggable": "^4.4.5",
    "react-icons": "^4.4.0",
    "react-media-recorder": "^1.6.5",
    "react-rte": "^0.16.5",
    "react-toastify": "^9.0.4",
    "sharp": "^0.30.6",
    "superjson": "^1.9.1"
  },
  "devDependencies": {
    "@types/node": "17.0.42",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/react-rte": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-typescript": "^0.14.0",
    "lint": "^0.7.0",
    "postcss": "^8.4.14",
    "prisma": "^3.15.1",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.8.1",
    "typescript": "4.7.3"
  }
}
